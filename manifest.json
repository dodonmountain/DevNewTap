{
    "manifest_version": 2,

    "name": "Getting started example",
    "description": "This extension shows a Google Image search result for the current page",
    "version": "1.0",
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "content_scripts": [{
        "matches": ["*://newtab/*"],
        "run_at": "document_end",
        "css":        ["style.css"],
        "js":         ["logic.js"]
    }],
    "chrome_url_overrides": {
        "newtab": "popup.html"
    },
    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*"
    ]
}
